<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.applicationsguessinggame">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar">
        <activity
            android:name="com.example.applicationsguessinggame.LegendActivity"
            android:exported="true" />
        <activity
            android:name="com.example.applicationsguessinggame.AmateurActivity"
            android:exported="true" />
        <activity
            android:name="com.example.applicationsguessinggame.noobActivity"
            android:exported="true" />
        <activity
            android:name="com.example.applicationsguessinggame.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>